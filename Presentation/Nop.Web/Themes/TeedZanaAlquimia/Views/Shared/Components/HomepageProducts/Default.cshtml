@model IList<ProductOverviewModel>
@if (Model.Count > 0)
{
    <div class="section">
        @*<h4>@T("HomePage.Products")</h4>*@

        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col s6 m6 l3" style="margin-bottom:2%;">
                    @await Html.PartialAsync("_ProductBox", item)
                </div>
            }
        </div>
    </div>
}
