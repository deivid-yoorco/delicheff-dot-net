@model IList<PollModel>
@if (Model.Count > 0)
{
    <div class="section">
        <h4>@T("Polls.Title")</h4>
        <div class="row">
            @foreach (var poll in Model)
            {
                <div class="col s6">
                    @await Html.PartialAsync("_Poll", poll)
                </div>
            }
        </div>
    </div>

}
