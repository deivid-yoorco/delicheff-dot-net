@model Teed.Plugin.Groceries.Models.UrbanPromoter.UrbanPromoterModel

@{
    Layout = "_AdminLayout";
    //active menu item (system name)
    Html.SetActiveMenuItemSystemName("Groceries.UrbanPromoters");
}

<form method="post" enctype="multipart/form-data">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            Editar promotor urbano - @Model.AccountOwnerName
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a href="/Admin/UrbanPromoter/View?id=@Model.Id">regresar a la lista de promotores</a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
            @*<span id="payrollemployee-delete" class="btn bg-red">
                    <i class="fa fa-trash-o"></i>
                    @T("Admin.Common.Delete")
                </span>*@
        </div>
    </div>
    @await Html.PartialAsync("~/Plugins/Teed.Plugin.Groceries/Views/UrbanPromoter/_CreateOrUpdate.cshtml", Model)
</form>
@*<nop-delete-confirmation asp-model-id="@Model.Id" asp-button-id="payrollemployee-delete" />*@