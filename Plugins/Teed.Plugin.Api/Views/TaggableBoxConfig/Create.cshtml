@model Teed.Plugin.Api.Models.TaggableBoxModel
@{
    Layout = "_AdminLayout";
    Html.SetActiveMenuItemSystemName("TeedApi.TaggableBoxes");
}
<form method="post" enctype="multipart/form-data">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            Agregar caja etiquetable
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="List">regresar a la lista de cajas</a>
            </small>
        </h1>
        <div class="pull-right">
            <button disabled type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button disabled type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>

    <div class="content">
        <div class="form-horizontal">
            <nop-tabs id="popup">
                <nop-tab asp-name="tab-slider" asp-title="Información" asp-default="true">
                    @await Html.PartialAsync("~/Plugins/Teed.Plugin.Api/Views/TaggableBoxConfig/_CreateOrUpdate.cshtml", Model)
                </nop-tab>
            </nop-tabs>
        </div>
    </div>
</form>