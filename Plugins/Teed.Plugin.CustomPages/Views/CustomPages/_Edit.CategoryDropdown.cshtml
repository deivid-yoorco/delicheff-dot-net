@model Teed.Plugin.CustomPages.Models.CustomPages.EditViewModel
@{
    var imageText = "El tamaño recomendado es 1980x800 píxeles.";
    var colorText = "El valor del texto debe ser escrito en código hexadecimal. Por ej: #C1C7C9";
}
<div class="panel panel-default">
    <div class="panel-heading">
        Imagen en fondo
    </div>
    <div class="panel-body">
        <div class="form-group">
            <div class="col-md-3">
                <nop-label asp-for="CatPicture6Id" />
            </div>
            <div class="col-md-9">
                <nop-editor asp-for="CatPicture6Id" />
                <span asp-validation-for="CatPicture6Id"></span>
                <em>@imageText</em>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-3 text-right">
                <label>Título</label>
            </div>
            <div class="col-md-9">
                <nop-editor asp-for="CatText6" />
                <span asp-validation-for="CatText6"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-3 text-right">
                <label>Color del texto</label>
            </div>
            <div class="col-md-9">
                <nop-editor asp-for="CatTextColor6" />
                <span asp-validation-for="CatTextColor6"></span>
                <em>@colorText</em>
            </div>
        </div>
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-heading">
        Opción 1
    </div>
    <div class="panel-body">
        <div class="form-group">
